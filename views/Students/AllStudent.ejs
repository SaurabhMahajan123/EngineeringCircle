<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link
    href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.0/dist/css/bootstrap.min.css"
    rel="stylesheet"
  />
  <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.0/dist/js/bootstrap.bundle.min.js"></script>
  <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.6.0/jquery.min.js"></script>
  <link rel="stylesheet" href="CSS/styleSheet.css" />
  
    <title>EngineeringCircle-Admin-Students</title>
</head>
<body>

    <div class="container-fluid">
        <div class="row text-center bg-light">
          <div class="col" id="adminPortal">Admin Portal</div>
        </div>
  
      
  
        <div class="row">
          <div class="col text-light" id="sideBar">
            <%-include('../AdminHeader&SideBar.ejs')%>
          </div>
  
          <div class="col col-lg-9 text-center adminWorkArea">
  
        <h3 style="margin-top: 5%;">Student List</h3>


            <table class="table table-bordered table-striped text-center w-75 mx-auto table-secondary border-1 my-4">
              <thead class="table-dark text-light">
                <th>StudentId</th>
                <th>Name</th>
                <th>Email</th>
                <th>Mobile</th>
                <th>Address</th>
                <th>Center</th>
              </thead>
  
             
              <tbody>
  
                 <% for(let i=0;i<all.length;i++){%>
                  <tr>
                <td><%= all[i].s_id %></td>   
                <td><%= all[i].s_name %></td>
                <td><%= all[i].s_email %></td>
                <td><%= all[i].s_mob_no %></td>
                <td><%= all[i].s_address %></td>
                <td><%= all[i].s_center_id %></td>

              </tr>
                 <%}%> 
              </tbody>
            </table>


             
   
          </div>
        </div>
      </div>

    <script src="script.js"></script>
</body>
</html>